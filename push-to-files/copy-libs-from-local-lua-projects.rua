#!/usr/bin/env rua
require 'config'
for projectName, projectLibs in pairs(libs) do
	for _,libLoadName in ipairs(projectLibs) do
		local libFileName = 'lib'..libLoadName..'.so'
		assert(os.exec('cp'
			..' '..path(assert(os.getenv'LUA_PROJECT_PATH')..'/'..projectName..'/bin/Android/arm/'..libFileName):escape()
			..' '..path(libFileName):escape()
		))
	end
end
